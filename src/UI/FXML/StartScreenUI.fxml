<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.Controllers.StartScreenUIController" fx:id="rootPane">
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" prefHeight="70.0" prefWidth="70.0" text="Sign UP" fx:id="signUp" onAction="#signUpEventHandler"/>
            <Button mnemonicParsing="false" prefHeight="70.0" prefWidth="70.0" text="Sign In" fx:id="signIn" onAction="#signInEventHandler" />
            <Button mnemonicParsing="false" prefHeight="70.0" prefWidth="70.0" text="Help" fx:id="help"/>
            <Button mnemonicParsing="false" prefHeight="70.0" prefWidth="70.0" text="Button" fx:id="exit"/>
         </children>
      </VBox>
   </center>
   <top>
      <Label prefHeight="97.0" prefWidth="369.0" text="Hello, Welcome to Mega Bank" textAlignment="CENTER" textOverrun="CLIP" wrapText="true" BorderPane.alignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </top>
</BorderPane>
